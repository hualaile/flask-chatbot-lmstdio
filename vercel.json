{
    "version": 2,
    "builds": [
      {
        "src": "api/chat.py",
        "use": "@vercel/python"
      },
      {
        "src": "templates/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "static/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "templates/index.html"
      },
      {
        "src": "/api/chat",
        "dest": "api/chat.py"
      }
    ]
  }
  